{
  "name": "boxcric-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server/index.js",
    "build": "vite build",
    "build:render": "npm run build && npm run build:server",
    "build:server": "echo 'Server files are ready'",
    "build:netlify": "npm run build && npm run copy-public",
    "copy-public": "cp -r public/* dist/ 2>/dev/null || echo 'No public files to copy'",
    "start": "node server/index.js",
    "start:render": "NODE_ENV=production node server/index.js",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "test:render": "node test-render-deployment.js",
    "deploy": "node deploy-to-render.js",
    "deploy:vercel": "node deploy-vercel.js",
    "check:health": "node check-deployment-health.js",
    "deploy:safe": "node deploy-with-fix.js",
    "postinstall": "echo 'Skipping puppeteer installation'"
  },
  "resolutions": {
    "@rollup/rollup-linux-x64-gnu": "^3.0.0"
  },
